programa
{
	cadeia branco
	funcao inicio()
	{
		para (inteiro planeta = 0; planeta <= 2; planeta += 1) {
			escreva ("Escaneando planeta ", planeta + 1, "...")
			leia (branco)
			limpa()
			escreva ("Planeta ", planeta + 1, " escaneado!")
			leia (branco)
			limpa()
		}
		escreva ("Programa desenvolvido por Bernardo")
	}
}
